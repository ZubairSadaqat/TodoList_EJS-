
<%- include("header")-%>
<h1 class="box" id="heading">   <%= listTitle %> </h1>

<div class="box">
    <% for (var i= 0 ; i< NewItemlists.length ; i++) {%>
        <div class="item">
            <input type="checkbox">
            <p> <%= NewItemlists[i]%> </p>
        </div>
    <%}%>
        <form class="item" action="/" method="post" >
            <input type="text" name="newItem" placeholder="NewItem" autocomplete="off">
            <button type="submit" name="list" value="<%=listTitle%>">+</button>
        </form>
</div>

<%- include("footer")-%>